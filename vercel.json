{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    },
    {
      "src": "/controller/controller.js",
      "use": "@now/node"
    },
    {
      "src": "model/model.js",
      "use": "@now/node"
    }

  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}